{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "es2022",
    "moduleResolution": "bundler",
    "types": ["svelte", "node", "vite"]
  },
  "include": ["src", "drizzle.config.ts"]
}